<option value="like">Like</option>

</select>
Enter Value:
<input type="text" name="fcond">

<INPUT Type="BUTTON" Value="Refresh" onClick="reloadPageFilter(this.form)">
</form>
<script>
var sel1 = document.frm.sel1;
